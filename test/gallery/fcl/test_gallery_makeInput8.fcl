#include "messageDefaults.fcl"

process_name: PROD4

services:
{
  scheduler:
  {
    wantSummary: false
  }
}
services.message: @local::messageDefaults

source:
{
  module_type: RootInput
  fileNames: [ "test_gallery7.root" ]
}

physics:
{
  filters:
  {
    f1:
    {
      module_type: FailEverything
    }
  }
  p1: [ f1 ]
  e1: [ output1 ]

  trigger_paths: [ p1 ]
  end_paths: [ e1 ]
}

outputs:
{
  output1:
  {
    module_type: RootOutput
    fileName: "test_gallery8.root"
    SelectEvents: { SelectEvents: [p1] }
  }
}
